# mes "one of us!";
#: npc/jobs/2-2/crusader.txt
msgctxt "Senior Crusader"
msgid "one of us!"
msgstr ""

# mes "...Brother.";
#: npc/jobs/2-2/crusader.txt
msgctxt "Senior Crusader"
msgid "...Brother."
