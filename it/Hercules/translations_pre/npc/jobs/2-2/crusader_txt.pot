# mes "What do you want...?";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "What do you want...?"
msgstr ""

# mes "If you have no business here,";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "If you have no business here,"
msgstr ""

# mes "then please leave. A tranquil state of mind is essential in self training...";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "then please leave. A tranquil state of mind is essential in self training..."
msgstr ""

# mes "I wish to improve the sense of serenity in my heart in preparation for the Holy War that is to come. So please, do not disturb me.";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "I wish to improve the sense of serenity in my heart in preparation for the Holy War that is to come. So please, do not disturb me."
msgstr ""

# mes "What is it...?";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "What is it...?"
msgstr ""

# mes "Do you have business";
#: npc/jobs/2-2/crusader.txt
