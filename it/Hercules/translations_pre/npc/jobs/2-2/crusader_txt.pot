msgstr ""

# mes "You wish to become a Crusader...? Hm, fighting in the Holy War is an admirable goal we may both share.";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "You wish to become a Crusader...? Hm, fighting in the Holy War is an admirable goal we may both share."
msgstr ""

# mes "Your name is";
#: npc/jobs/2-2/crusader.txt
msgctxt "Man in Anguish"
msgid "Your name is"
