#: npc/re/quests/magic_books.txt
# select("Yes, I knew this was coming!");
msgctxt "Master Velofos"
msgid "Yes, I knew this was coming!"
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "My brother is just like you. He's very interested in recreating the Ultimate Magic Book.";
msgctxt "Master Velofos"
msgid "My brother is just like you. He's very interested in recreating the Ultimate Magic Book."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "His name is Galfos.";
msgctxt "Master Velofos"
msgid "His name is Galfos."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "He's been taking a great interest in the ancient Magic Books.";
msgctxt "Master Velofos"
msgid "He's been taking a great interest in the ancient Magic Books."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "I didn't support him or give him any advice for his research since I thought he'd give on it pretty quickly, but...";
msgctxt "Master Velofos"
msgid "I didn't support him or give him any advice for his research since I thought he'd give on it pretty quickly, but..."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "If he's discovered anything useful, maybe it can helpful to you.";
msgctxt "Master Velofos"
msgid "If he's discovered anything useful, maybe it can helpful to you."
msgstr ""

#: npc/re/quests/magic_books.txt
# select("Where's Galfos now?");
msgctxt "Master Velofos"
msgid "Where's Galfos now?"
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "I've heard that he's near the Expedition Camp beyond the Dimensional Rift to seek information about the ancient Magic Books.";
msgctxt "Master Velofos"
msgid "I've heard that he's near the Expedition Camp beyond the Dimensional Rift to seek information about the ancient Magic Books."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "If you want to see him, you should prepare for a long journey.";
msgctxt "Master Velofos"
msgid "If you want to see him, you should prepare for a long journey."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "Galfos is near the Expedition Camp beyond the Dimensional Rift.";
msgctxt "Master Velofos"
msgid "Galfos is near the Expedition Camp beyond the Dimensional Rift."
msgstr ""

#: npc/re/quests/magic_books.txt
# mes "Ha ha, I'm sorry, but you should focus on strengthening your body rather than creating Magic Books.";
msgctxt "Master Velofos"
msgid "Ha ha, I'm sorry, but you should focus on strengthening your body rather than creating Magic Books."
msgstr ""

#: npc/re/quests/magic_books.txt
