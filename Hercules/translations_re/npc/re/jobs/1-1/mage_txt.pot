msgstr ""

#: npc/re/jobs/1-1/mage.txt
# mes "Wanna be a Mage? Eh...";
msgctxt "Mage Guildsman#mage"
msgid "Wanna be a Mage? Eh..."
msgstr ""

#: npc/re/jobs/1-1/mage.txt
# mes "Hey, look at you! You're kinda cute~! Not my type though...";
msgctxt "Mage Guildsman#mage"
msgid "Hey, look at you! You're kinda cute~! Not my type though..."
msgstr ""

#: npc/re/jobs/1-1/mage.txt
# mes "Oooh, you're such a hot babe~!";
msgctxt "Mage Guildsman#mage"
msgid "Oooh, you're such a hot babe~!"
msgstr ""

#: npc/re/jobs/1-1/mage.txt
# mes "I like girls like you~";
msgctxt "Mage Guildsman#mage"
msgid "I like girls like you~"
msgstr ""

#: npc/re/jobs/1-1/mage.txt
# mes "Right, you said that you wanna be a Mage?";
msgctxt "Mage Guildsman#mage"
msgid "Right, you said that you wanna be a Mage?"
msgstr ""

