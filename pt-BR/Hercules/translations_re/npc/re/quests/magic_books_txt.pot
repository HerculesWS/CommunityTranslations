msgid "I'm not here to babysit. Get lost!"
msgstr ""

# mes "Ha ha, are you also seeking new knowledge?";
#: npc/re/quests/magic_books.txt
msgctxt "Master Velofos"
msgid "Ha ha, are you also seeking new knowledge?"
msgstr ""

